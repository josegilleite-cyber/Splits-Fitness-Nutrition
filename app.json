{
  "expo": {
    "name": "Splits Fitness & Nutrition",
    "slug": "splits-fitness-nutrition",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.splits.fitnessnutrition",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This app does not track you. This permission is never requested.",
        "NSHealthShareUsageDescription": "This app does not access health data. All data is stored locally.",
        "NSHealthUpdateUsageDescription": "This app does not access health data. All data is stored locally."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.splits.fitnessnutrition",
      "permissions": [
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./app.plugin.js",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ],
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "38fd694f-8be1-4a0c-a513-8a9c0d5c8ed6"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/38fd694f-8be1-4a0c-a513-8a9c0d5c8ed6"
    }
  }
}
